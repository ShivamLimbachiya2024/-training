<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Document</title>
        <style>
              
              body{
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
        }
        table tr th{
                color: white;
                background-color: black;
            }
            table tr td{
                border: 1px solid black;
            }
        </style>
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
        <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    </head>

    <body>
        <h1>Shivam Limbachiya</h1>
        <%var keyObj=Object.keys(data[0]);%>
        <%var terminaltotalmarks=0%>
        <%var primilarytotalmarks=0%>
        <%var finaltotalmarks=0%>
        <%var practicaltotalmarks=0%>
        <%var theoryTotalmarks=0%>
        <%var totalmarks=0%>
        <table>
            <tr>
                <%for(let i=0;i<keyObj.length;i++){%>
                    <th><%=keyObj[i]%></th>
                    <%}%>
                </tr>
                <%for(let i=0;i<data.length;i++){%>
                    <tr>
                    <%for(let j=0;j<keyObj.length;j++){%>
                        <% 
                        if(keyObj[j]=="TerminalTheoryMarks" || keyObj[j]=="TerminalPracticalMarks" ){
                            terminaltotalmarks=terminaltotalmarks+Number(data[i][keyObj[j]])
                        }else if(keyObj[j]=="PrimilaryTheory" || keyObj[j]=="PrimilaryPractical"){
                            primilarytotalmarks=primilarytotalmarks+Number(data[i][keyObj[j]])
                        }else if(keyObj[j]=="FinalTheory" || keyObj[j]=="FinalPractical"){
                            finaltotalmarks=finaltotalmarks+Number(data[i][keyObj[j]])
                        }else if(keyObj[j]=="TotalTheoryMarks"){
                                theoryTotalmarks=theoryTotalmarks+Number(data[i][keyObj[j]])
                        }else if(keyObj[j]=="TotalPracticalMarks"){
                                practicaltotalmarks=practicaltotalmarks+Number(data[i][keyObj[j]])
                        }else if(keyObj[j]=="total"){
                            totalmarks=totalmarks+Number(data[i][keyObj[j]])
                        }
                            %>
                            <td><%=data[i][keyObj[j]]%></td>
                            
                            <%}%>                    
                        </tr>
                        <%}%>
                        <tr>
                            <td>
                                percentage
                            </td>
                            <td colspan="2">
                                <%=(terminaltotalmarks/6).toFixed(2)%>
                            </td>
                            <td colspan="2">
                                <%=(primilarytotalmarks/6).toFixed(2)%>
                            </td>
                            <td colspan="2">
                                <%=(finaltotalmarks/6).toFixed(2)%>
                            </td>
                            <td>
                                <%=(theoryTotalmarks).toFixed(2)%>
                            </td>
                            <td>
                                <%=(practicaltotalmarks).toFixed(2)%>
                            </td>
                            <td>
                                <%=(totalmarks/12).toFixed(2)%>
                            </td>
                        </tr>
                    </table>
                   
                </body>
</html>